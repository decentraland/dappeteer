{
  "name": "dappeteer",
  "version": "1.1.0",
  "description": "E2E testing for dApps using Puppeteer + MetaMask",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "metamask",
    "dist"
  ],
  "scripts": {
    "postinstall": "npx rimraf node_modules/**/**/.git",
    "build": "tsc",
    "prepare": "npm run build",
    "test": "mocha -r ts-node/register test/test.ts --timeout=200000 --exit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/decentraland/dappeteer.git"
  },
  "keywords": [
    "e2e",
    "testing",
    "metamask",
    "puppeteer",
    "dapp",
    "ethereum"
  ],
  "author": "Juan Cazala <juan@decentraland.org>",
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@types/puppeteer": "^2.1.5",
    "dcl-tslint-config-standard": "^2.0.0",
    "ganache-core": "^2.13.2",
    "mocha": "7.2.0",
    "puppeteer": "^2.1.1",
    "rocketh": "https://github.com/wighawag/rocketh.git#3bcfdb3b7d38ac98449b3d5075e12868b32d5a0c",
    "serve-handler": "5.0.8",
    "solc": "0.5.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "web3": "1.2.6"
  },
  "peerDependencies": {
    "puppeteer": "^2.1.1"
  }
}
